package javamainl;
import java.util.Scanner;
import java.util.Arrays;

public class javapractice {

	public static void test() {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] array = new int[n];
		int sum = 0;
		for(int i = 0; i <n ;i++) {
			array[i] = sc.nextInt();
			sum += array[i];
		}
		System.out.println(sum);
		
	}

	public static void main(String[] args) {
		test();
	}			
}
